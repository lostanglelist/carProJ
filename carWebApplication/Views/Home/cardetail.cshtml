@{
    ViewData["Title"] = "cardetail";
}
<main id="main">

    <!-- Head Section -->
    <section class="small-section bg-gray-lighter">
        <div class="relative container align-left">

            <div class="row">

                <div class="col-md-8">
                    <h1 class="hs-line-11 font-alt mb-20 mb-xs-0">Single Product</h1>
                    <div class="hs-line-4 font-alt black">
                        Choose the best products in our shop
                    </div>
                </div>

                <div class="col-md-4 mt-30">
                    <div class="mod-breadcrumbs font-alt align-right">
                        <a href="#">Home</a>&nbsp;/&nbsp;<a href="#">Shop</a>&nbsp;/&nbsp;<span>Single</span>
                    </div>

                </div>
            </div>

        </div>
    </section>
    <!-- End Head Section -->
    <!-- Section -->
    <section class="page-section">
        <div class="container relative">

            <!-- Product Content -->
            <div class="row mb-60 mb-xs-30">

                <!-- Product Images -->
                <div class="col-md-4 mb-md-30">

                    <div class="post-prev-img">
                        <img src="~/images/car/corvett_c8.jpg" />
                        
                        
                        <div class="intro-label">
                            <span class="badge badge-danger bg-red">Sale</span>
                        </div>
                    </div>

                    
                </div>
                <!-- End Product Images -->
                <!-- Product Description -->
                <div class="col-sm-8 col-md-5 mb-xs-40">

                    <h3 class="mt-0">Chevrolet Corvett Stringray C8</h3>

                    <hr class="mt-0 mb-30" />

                    <div class="row">
                        <div class="col-6 lead mt-0 mb-20">

                            
                            <strong>12,000,000 ฿</strong>

                        </div>
                      
                    </div>

                    <hr class="mt-0 mb-30" />

                    <ul>
                        <li style="font-weight: 400;" aria-level="1"><span style="font-weight: 400;">6.2L V8 LT2 engine</span></li>
                        <li style="font-weight: 400;" aria-level="1"><span style="font-weight: 400;">490 horsepower</span></li>
                        <li style="font-weight: 400;" aria-level="1"><span style="font-weight: 400;">465 lb-ft of torque</span></li>
                        <li aria-level="1">Power-retractable seats</li>
                        <li aria-level="1">Rear Camera Mirror Rear park assist and HD rear vision camera</li>
                        <li aria-level="1">Bose premium 10-speaker system</li>
                        <li aria-level="1">Chevrvolet Infotainment 3 system</li>
                        
                    </ul>

                    <hr class="mt-0 mb-30" />

                    <div class="mb-30">
                        <form asp-controller="carcal" asp-action="car" method="post" class="form">
                            
                            Money : <input type="number" name="minMoney" class="input-lg round" min="120000" placeholder="min = 120,000 ฿" /> <br /><br />
                            <input type="number" class="input-lg round" min="1" max="12" value="1" />
                            <a href="#" class="btn btn-mod btn-large btn-round">Add to Cart</a>
                            
                            <input type="submit" value="calculate" />
                            <p>@ViewBag.result</p>
                        </form>
                        
                    </div>

                    <hr class="mt-0 mb-30" />

                  

                </div>
                <!-- End Product Description -->
                <!-- Features -->
                <div class="col-sm-4 col-md-3 mb-xs-40">

                    <!-- Features Item -->
                    <div class="alt-service-wrap">
                        <div class="alt-service-item">
                            <div class="alt-service-icon">
                                <i class="fa fa-paper-plane-o"></i>
                            </div>
                            <h3 class="alt-services-title font-alt">Free Shipping</h3>
                            <br />
                        </div>
                    </div>
                    <!-- End Features Item -->
                    <!-- Features Item -->
                    <div class="alt-service-wrap">
                        <div class="alt-service-item">
                            <div class="alt-service-icon">
                                <i class="fa fa-clock-o"></i>
                            </div>
                            <h3 class="alt-services-title font-alt">14 days moneyback</h3>
                           <br />
                        </div>
                    </div>
                    <!-- End Features Item -->
                    <!-- Features Item -->
